--------------------------------------------------------
--  DDL for Trigger LOGS_WYNAGRODZENIE
--------------------------------------------------------

  CREATE OR REPLACE TRIGGER "ADMIN"."LOGS_WYNAGRODZENIE" AFTER UPDATE
ON zatrudnienie
FOR EACH ROW
BEGIN    
    IF :OLD.wynagrodzenie != :NEW.wynagrodzenie THEN
    INSERT INTO wynagrodzenie_logs(id_pracownik,old_wynagrodzenie,new_wynagrodzenie)
        VALUES(
            :OLD.pracownik_ID,
            :OLD.wynagrodzenie,
            :NEW.wynagrodzenie
        );
        END IF;
END;
/
ALTER TRIGGER "ADMIN"."LOGS_WYNAGRODZENIE" ENABLE;
--------------------------------------------------------
--  DDL for Trigger PRACOWNIK_TRG
--------------------------------------------------------

  CREATE OR REPLACE TRIGGER "ADMIN"."PRACOWNIK_TRG" 
BEFORE INSERT ON PRACOWNIK 
FOR EACH ROW 
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.ID IS NULL THEN
      SELECT PRACOWNIK_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
    END IF;
  END COLUMN_SEQUENCES;
END;
/
ALTER TRIGGER "ADMIN"."PRACOWNIK_TRG" ENABLE;
--------------------------------------------------------
--  DDL for Trigger VALIDATE_PRACOWNIK
--------------------------------------------------------

  CREATE OR REPLACE TRIGGER "ADMIN"."VALIDATE_PRACOWNIK" BEFORE INSERT OR UPDATE ON pracownik
FOR EACH ROW
BEGIN
    IF LENGTH(:NEW.imie) < 2 THEN
        raise_application_error(-20002, 'Zbyt krotkie imie!');
    END IF;

    :NEW.imie := initcap(:NEW.imie);
    :NEW.nazwisko := initcap(:NEW.nazwisko);

    IF :NEW.plec ='m' then
    :NEW.plec := 'M';
    END IF;

    IF :NEW.plec = 'k' then
    :NEW.plec := 'K';
    END IF;
END;
/
ALTER TRIGGER "ADMIN"."VALIDATE_PRACOWNIK" ENABLE;
--------------------------------------------------------
--  DDL for Trigger VALIDATE_PRACOWNIK_SECON_NAME
--------------------------------------------------------

  CREATE OR REPLACE TRIGGER "ADMIN"."VALIDATE_PRACOWNIK_SECON_NAME" BEFORE INSERT ON pracownik
FOR EACH ROW
BEGIN
    IF :NEW.IMIE = :NEW.DRUGIE_IMIE THEN
    raise_application_error(-20002, 'Pierwsze i drugie imie nie moga byc takie same!');
    END IF;
END;
/
ALTER TRIGGER "ADMIN"."VALIDATE_PRACOWNIK_SECON_NAME" ENABLE;
--------------------------------------------------------
--  DDL for Trigger VALIDATE_WYNAGRODZENIE
--------------------------------------------------------

  CREATE OR REPLACE TRIGGER "ADMIN"."VALIDATE_WYNAGRODZENIE" BEFORE INSERT OR UPDATE ON zatrudnienie
FOR EACH ROW
BEGIN
    IF :NEW.WYNAGRODZENIE < 0 THEN
    raise_application_error(-20002, 'Wynagrodzenie nie moze byc ujemne!');
    END IF;
END;
/
ALTER TRIGGER "ADMIN"."VALIDATE_WYNAGRODZENIE" ENABLE;
