--------------------------------------------------------
--  DDL for Function AKTUALNA_PENSJA
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."AKTUALNA_PENSJA" (id_osoba number) return number is 
    wynik number(8);
begin 
    select wynagrodzenie into wynik
    from zatrudnienie
    where pracownik_id = id_osoba;

    return wynik;
end;

/
--------------------------------------------------------
--  DDL for Function LICZBA_PRACOWNIKOW
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."LICZBA_PRACOWNIKOW" 
RETURN NUMBER
AS
    x int;
    BEGIN
    select count(*) INTO x from pracownik;
    RETURN x;
END liczba_pracownikow;

/
--------------------------------------------------------
--  DDL for Function PENSJA_KIEROWNIKA
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."PENSJA_KIEROWNIKA" (id_stanowisko number) return number is
    wynik number(8,2);
begin
    select zatrudnienie.wynagrodzenie into wynik
    from zatrudnienie where stanowisko_id = id_stanowisko;

    return wynik;

    exception
        when no_data_found then return 0;
end;

/
--------------------------------------------------------
--  DDL for Function PENSJA_POWYZEJ
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."PENSJA_POWYZEJ" (id_dzialu NUMBER, prog number) return number is
    razem number(10);
    wybrani number(10);
    wynik number(5,2) := 0;
begin
    select count (*) into razem from zatrudnienie
    where dzial_id = id_dzialu;

    select count(*) into wybrani from zatrudnienie
    where wynagrodzenie > prog and dzial_id = id_dzialu;

    if wybrani > 0 and razem > 0 THEN
        wynik := wybrani / razem * 100;
    end if;
    return wynik;
END;

/
--------------------------------------------------------
--  DDL for Function WYZNACZ_PLEC
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."WYZNACZ_PLEC" (kod_plci char) return varchar2 is 
    plec varchar2(20);
begin
    if kod_plci = 'K' then
        plec := 'Kobieta';
    elsif kod_plci = 'M' then
        plec := 'Mezczyzna';
    else
        plec := 'Inna';
     end if;   
    return plec;
end;

/
--------------------------------------------------------
--  DDL for Function ZNAJDZ_DZIAL
--------------------------------------------------------

  CREATE OR REPLACE FUNCTION "ADMIN"."ZNAJDZ_DZIAL" (dzial_id number)
RETURN VARCHAR
AS
    nazwa_dzialu varchar(50);
BEGIN
    select nazwa INTO nazwa_dzialu
        from dzial
            where id = dzial_id;
RETURN nazwa_dzialu;
END znajdz_dzial;

/
